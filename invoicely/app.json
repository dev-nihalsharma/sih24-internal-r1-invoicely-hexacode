{
  "expo": {
    "name": "invoicely",
    "slug": "invoicely",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-icon-transparent.png",
    "scheme": "invoicely",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-icon-transparent.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nihalsharma007.invoicely"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-icon-transparent.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "963ea166-7482-47cd-97df-d51aebb79d0e"
      }
    }
  }
}
